package exo;

public class ChatMessage {
	private String hostName ; 
	private boolean statut ; 

	public ChatMessage( String ip, boolean connecte ){
		this.hostName = ip ; 
		this.statut = connecte ; 
	}

	public String getName() {
		return this.hostName ; 
	}
	public boolean getStatut() {
		return this.statut ; 
	}
}

package exo;

import java.net.Socket;

public class Server {
	static class ServerThread implements Runnable {
		Socket client = null ; 

		public ServerThread(Socket c) {
			this.client = c;
		}
		public void run() {
			try {
				System.out.println("Connected to client : "+client.getInetAddress().getHostName());
				client.close();
			} catch (Exception e) {
				System.err.println(e.getMessage());
			}
		}
	}
}







